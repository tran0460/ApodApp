<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="ApodTestApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:ApodTestApp"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        
    
        <ShellContent
            Title="Home"
            ContentTemplate="{DataTemplate local:MainPage}"
            Route="MainPage" />        
        
        <ShellContent
            Title="SlideShow"
            Icon="display.png"
            ContentTemplate="{DataTemplate local:SlideShowPage}"
            Route="SlideShow" />
    </TabBar>

</Shell>
